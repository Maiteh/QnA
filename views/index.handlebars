<h2 class="discussions">Discussions</h2>

{{#each data}}
    <div class="wrapper-discussions">
        <a href="discussions/{{this.id}}">
            <div class="title-discussion">{{this.title}}</div>
            <div class="msg-discussion">{{this.message}}</div>
            <div class="msg-discussion-user">{{this.username}}</div>
        </a>
    </div>
{{/each}}
